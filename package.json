{
  "name": "nekro-vstack",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "bash scripts/dev-backend.sh",
    "dev:frontend": "vite",
    "dev:watch": "node scripts/watch-openapi.js",
    "dev:all": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\" \"pnpm dev:watch\" --names \"nvs-bed,nvs-fed,nvs-types\" --prefix-colors \"blue,green,yellow\"",
    "build": "vite build",
    "build:backend": "echo 'Backend needs deployment setup'",
    "lint": "eslint .",
    "lint:backend": "uv run ruff check src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:backend": "uv run ruff format src/",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "db:generate": "uv run aerich migrate",
    "db:migrate": "uv run aerich upgrade",
    "db:rollback": "uv run aerich downgrade",
    "db:history": "uv run aerich history",
    "db:init": "uv run aerich init -t src.backend.config.database.TORTOISE_ORM",
    "db:init-db": "uv run aerich init-db",
    "generate:openapi": "uv run python scripts/generate-openapi.py",
    "generate:types": "pnpm generate:openapi && openapi-typescript openapi.json -o src/frontend/core/types/generated.ts",
    "generate:types:server": "openapi-typescript http://localhost:9871/openapi.json -o src/frontend/core/types/generated.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/inter": "^5.2.8",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/lab": "6.0.0-beta.22",
    "@mui/material": "^6.3.1",
    "@mui/utils": "^6.4.6",
    "@mui/x-data-grid": "^7.23.6",
    "@tanstack/react-query": "^5.63.0",
    "@types/react-virtualized-auto-sizer": "^1.0.8",
    "@types/react-window": "^1.8.8",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.17.1",
    "immer": "^10.1.1",
    "notistack": "^3.0.1",
    "rc-slider": "^11.1.8",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-intersection-observer": "^9.15.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.1.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.15.1",
    "remark-gfm": "^4.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@mui/types": "^7.2.21",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "openapi-typescript": "^7.4.4",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
